  const [section, setSection] = useState('');\n\n    const handleSubmit = async (e) => {\n        e.preventDefault();\n        try {\n            const response = await axios.post('/api/tasks', { title, description, section });\n            onTaskAdded(response.data);\n            setTitle('');\n            setDescription('');\n            setSection('');\n        } catch (error) {\n            console.error('Error adding task:', error);\n        }\n    };\n\n    return (\n        <form onSubmit={handleSubmit}>\n            <div>\n                <label>Title:</label>\n                <input\n                    type=\"text\"\n                    value={title}\n                    onChange={(e) => setTitle(e.target.value)}\n                    required\n                />\n            </div>\n            <div>\n                <label>Description:</label>\n                <textarea\n                    value={description}\n                    onChange={(e) => setDescription(e.target.value)}\n                    required\n                />\n            </div>\n            <div>\n                <label>Section:</label>\n                <input\n                    type=\"text\"\n                    value={section}\n                    onChange={(e) => setSection(e.target.value)}\n                    required\n                />\n            </div>\n            <button type=\"submit\">Add Task</button>\n        </form>\n    );\n};\n\nexport default TaskForm;","import React from 'react';\nimport TaskForm from './TaskForm';\n\nconst TaskSection = ({ title, tasks, onAddTask }) => {\n    return (\n        <div className=\"task-section\">\n            <h2>{title}</h2>\n            <TaskForm onAddTask={onAddTask} />\n            <ul>\n                {tasks.map((task) => (\n                    <li key={task.id}>\n                        <h3>{task.title}</h3>\n                        <p>{task.description}</p>\n                        <span>Status: {task.status}</span>\n                    </li>\n                ))}\n            </ul>\n        </div>\n    );\n};\n\nexport default TaskSection;","import React from 'react';\nimport TaskSection from './TaskSection';\n\nconst TaskList = ({ tasks }) => {\n    const sections = [...new Set(tasks.map(task => task.section))];\n\n    return (\n        <div>\n            {sections.map(section => (\n                <TaskSection \n                    key={section} \n                    sectionTitle={section} \n                    tasks={tasks.filter(task => task.section === section)} \n                />\n            ))}\n        </div>\n    );\n};\n\nexport default TaskList;","import React, { useState, useEffect } from 'react';\nimport TaskList from './components/TaskList';\nimport TaskForm from './components/TaskForm';\nimport axios from 'axios';\n\nconst App = () => {\n    const [tasks, setTasks] = useState([]);\n\n    useEffect(() => {\n        const fetchTasks = async () => {\n            const response = await axios.get('/api/tasks');\n            setTasks(response.data);\n        };\n        fetchTasks();\n    }, []);\n\n    const addTask = async (task) => {\n        const response = await axios.post('/api/tasks', task);\n        setTasks([...tasks, response.data]);\n    };\n\n    const updateTask = async (updatedTask) => {\n        const response = await axios.put(`/api/tasks/${updatedTask._id}`, updatedTask);\n        setTasks(tasks.map(task => (task._id === updatedTask._id ? response.data : task)));\n    };\n\n    const deleteTask = async (taskId) => {\n        await axios.delete(`/api/tasks/${taskId}`);\n        setTasks(tasks.filter(task => task._id !== taskId));\n    };\n\n    return (\n        <div>\n            <h1>Gestor de Tareas</h1>\n            <TaskForm addTask={addTask} />\n            <TaskList tasks={tasks} updateTask={updateTask} deleteTask={deleteTask} />\n        </div>\n    );\n};\n\nexport default App;","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport './index.css';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}